{% extends 'base.html.twig' %}

{% block title %}Musique{% endblock %}

{% block body %}
    <h1>Musique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ track.id }}</td>
            </tr>
            <tr>
                <th>Titres</th>
                <td>{{ track.title }}</td>
            </tr>
            <tr>
                <th>Durée</th>
                <td>{{ track.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_web_track_index') }}">Revenir à la liste</a>

    <a href="{{ path('app_web_track_edit', {'id': track.id}) }}">Modifier</a>

    {{ include('web/track/_delete_form.html.twig') }}
{% endblock %}
