<form method="post" action="{{ path('app_web_track_delete', {'id': track.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ track.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>